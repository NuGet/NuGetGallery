  <ScaffoldFile Path="WebPICmdLine.exe.content" TargetPath="bin\WebPICmdLine.exe" />
  <ScaffoldFile Path="Microsoft.Web.Deployment.dll.content" TargetPath="bin\Microsoft.Web.Deployment.dll" />
