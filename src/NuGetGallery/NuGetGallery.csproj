<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <UseNuGetBuildExtensions>true</UseNuGetBuildExtensions>
    <SkipValidatePackageReferences>true</SkipValidatePackageReferences>
    <OutputType>Library</OutputType>
    <AppDesignerFolder>Properties</AppDesignerFolder>
    <RootNamespace>NuGetGallery</RootNamespace>
    <AssemblyName>NuGetGallery</AssemblyName>
    <TargetFramework>net472</TargetFramework>
    <UseIISExpress>true</UseIISExpress>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <LangVersion>latest</LangVersion>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <IISExpressSSLPort>443</IISExpressSSLPort>
    <IISExpressAnonymousAuthentication>enabled</IISExpressAnonymousAuthentication>
    <IISExpressWindowsAuthentication>disabled</IISExpressWindowsAuthentication>
    <IISExpressUseClassicPipelineMode>false</IISExpressUseClassicPipelineMode>
    <CodeContractsAssemblyMode>0</CodeContractsAssemblyMode>
    <MvcProjectUpgradeChecked>true</MvcProjectUpgradeChecked>
    <UseGlobalApplicationHostFile />
    <MvcBuildViews Condition=" '$(MvcBuildViews)' == '' ">false</MvcBuildViews>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <Use64BitIISExpress />
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugSymbols>true</DebugSymbols>
    <DebugType>full</DebugType>
    <Optimize>false</Optimize>
    <DefineConstants>DEBUG;TRACE</DefineConstants>
    <ErrorReport>prompt</ErrorReport>
    <WarningLevel>4</WarningLevel>
    <Prefer32Bit>false</Prefer32Bit>
    <UseVSHostingProcess>true</UseVSHostingProcess>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType>pdbonly</DebugType>
    <Optimize>true</Optimize>
    <DefineConstants>TRACE</DefineConstants>
    <ErrorReport>prompt</ErrorReport>
    <WarningLevel>4</WarningLevel>
    <Prefer32Bit>false</Prefer32Bit>
  </PropertyGroup>
  <PropertyGroup>
    <PreBuildEvent>
      copy "$(MSBuildThisFileDirectory)..\Bootstrap\dist\css\bootstrap.min.css" "$(MSBuildThisFileDirectory)Content\gallery\css"
      copy "$(MSBuildThisFileDirectory)..\Bootstrap\dist\js\bootstrap.js" "$(MSBuildThisFileDirectory)Scripts\gallery"
    </PreBuildEvent>
  </PropertyGroup>
