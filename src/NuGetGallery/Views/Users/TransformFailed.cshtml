@{
    ViewBag.Title = "Transform Account";
    ViewBag.MdPageColumns = Constants.ColumnsFormMd;
    Layout = "~/Views/Shared/Gallery/Layout.cshtml";
}

<section role="main" class="container main-container page-account-transform-failed">
    <div class="row col-sm-8">
        @if (TempData.ContainsKey("TransformError"))
        {
            <h1>Failed to transform to an organization</h1>
            <hr />
            <div>
                @TempData["TransformError"]
            </div>
        }
    </div>
</section>