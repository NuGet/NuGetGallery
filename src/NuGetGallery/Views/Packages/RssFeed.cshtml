@model NuGetGallery.RssFeedViewModel
@{
    Layout = null;
}
<rss version="2.0">
    <channel>
        <title>NuGet Package '@Model.PackageId' Feed</title>
        <link>@Url.Package(Model.PackageId, false)</link>
        <description>@Model.PackageDescription</description>
        <lastBuildDate>@Model.PackageVersions.First().Created.ToUniversalTime().ToString("r")</lastBuildDate>
        <language>en-us</language>
        @foreach (var p in Model.PackageVersions)
        {
            <item>
                <title>@NuGetVersionFormatter.ToFullStringOrFallback(p.Version, fallback: p.Version);</title>
                @Html.Raw("<link>")
                @Url.Package(p, false)
                @Html.Raw("</link>")
                <guid>@Url.Package(p, false)</guid>
                <pubDate>@p.Created.ToUniversalTime().ToString("r")</pubDate>
                <description>@p.ReleaseNotes</description>
            </item>
        }
    </channel>
</rss>