<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\..\SdkProjects.props" />
  <PropertyGroup>
    <TargetFramework>net472</TargetFramework>
    <!-- Add Visual Studio assembly load path for the test framework assemblies. -->
    <AssemblySearchPaths Condition="'$(VSINSTALLDIR)' != ''">$(AssemblySearchPaths);$(VSINSTALLDIR)Common7\IDE\PublicAssemblies</AssemblySearchPaths>
    <AssemblySearchPaths Condition="'$(DevEnvDir)' != ''">$(AssemblySearchPaths);$(DevEnvDir)PublicAssemblies</AssemblySearchPaths>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="Microsoft.VisualStudio.QualityTools.WebTestFramework" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\NuGetGallery.FunctionalTests.Core\NuGetGallery.FunctionalTests.Core.csproj" />
  </ItemGroup>
</Project>